{
  "name": "server",
  "version": "0.1.0",
  "main": "src/app.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/app.ts",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && pnpm prisma generate",
    "db:update": "pnpm exec prisma db pull && pnpm prisma generate && pnpm exec prisma-case-format -p --file ./prisma/schema.prisma",
    "db:init": "pnpm ts-node-dev src/db/init.ts"
  },
  "devDependencies": {
    "@prisma/client": "^5.1.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.17.1",
    "@vitest/coverage-c8": "^0.30.1",
    "morgan": "^1.10.0",
    "prisma": "^5.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.31.4",
    "vitest-mock-extended": "^1.1.4"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.32.0",
    "@trpc/client": "^10.35.0",
    "@trpc/react-query": "^10.35.0",
    "@trpc/server": "^10.35.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.1",
    "pnpm": "^8.6.10",
    "prisma-case-format": "^1.7.2",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  }
}
