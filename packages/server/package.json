    "@vitest/coverage-c8": "^0.30.1",
    "morgan": "^1.10.0",
    "prisma": "^4.13.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1",
    "vitest-mock-extended": "^1.1.3"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@trpc/server": "^10.34.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cookie": "^0.5.0",
